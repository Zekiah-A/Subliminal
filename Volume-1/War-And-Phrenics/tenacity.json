    "tags": "Revenge, Love, Poem, Irony, Danger",
    "cWarning": false,
    "cWarningAdditions": "",
    "poemName": "Tenacity",
    "poemAuthor": "Zekiah",
    "poemContent": {
        "type": "fragment",
        "styles": [],
        "children": [
            {
                "type": "text",
                "content": "For the first day my eyes bisected every inch of your "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "personage, I gave you my energy, my all, my heart "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "my blood, my toil, my gall. Yet, it never returned, no, your "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "etheral glow, shut out by a blizzard of snow, "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "my body dropping, as my mind freezes, my fingers numbing "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "an epiphany. "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "I pull AKs; now you run, "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "bullets ripping through you, love, "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "you didn't give me shit, "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "It's no mistake. "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "Two weeks later, "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "I spit, "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "text",
                "content": "on your grave. "
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "newline",
                "count": 1
            },
            {
                "type": "newline",
                "count": 1
            }
        ]
    },
